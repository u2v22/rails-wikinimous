
<div class=container>
  <h1>wikinimous</h1>

  <% @articles.each do |article| %>
    <h3><%= link_to article.title, article_path(article) %></h3>
    <p><%= article.content %></p>
    <p><%= link_to "(delete)", article_path(article), method: :delete, data: { confirm: "Are you sure?" } %> </p>
  <% end %>
</div>
